{
  "extends": ["walmart/configurations/es5-test", "plugin:jest/recommended"],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "comma-dangle": "off",
    "one-var": "off",
    "func-style": "off",
    "no-unused-vars": "warn",
    "max-len": ["error", 120, 4],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-console": 1,
    "filenames/match-regex": [2, "^[a-z][a-z-A-Z0-9]+(\\.test)?$", true],
  }
}
